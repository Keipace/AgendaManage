<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fragmentTest_title_tv"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="敬请期待"
        android:textSize="@dimen/font_large_size"
        android:textColor="#C51162"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fragmentTest_groupTitle_tv"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fragmentTest_title_tv"
        app:layout_constraintBottom_toTopOf="@id/fragmentTest_groupMember_tv"
        app:layout_constraintVertical_chainStyle="packed"
        android:text="开发团队:"
        android:textSize="@dimen/font_size"
        android:textColor="@color/black"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fragmentTest_groupMember_tv"
        app:layout_constraintRight_toRightOf="@id/fragmentTest_groupTitle_tv"
        app:layout_constraintLeft_toLeftOf="@id/fragmentTest_groupTitle_tv"
        app:layout_constraintTop_toBottomOf="@id/fragmentTest_groupTitle_tv"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="李纪龙\n张世洋\n刘克寒\n李浩铭"
        android:textColor="@color/black"
        />

</androidx.constraintlayout.widget.ConstraintLayout>